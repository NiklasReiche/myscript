expression :=
    operation
    Id
    Number

operation :=
    operator
    (let Id expr expr)
    (fun Id* expr)
    (app Id expr*)
    (seq expr+)

operator :=
    (+ expression+)
    (- expression+)
    (* expression expression+)
    (/ expression expression+)
    (& expression+)
    (| expression+)
    (! expression)
